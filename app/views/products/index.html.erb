
<div class="wrapper">
  <div class="container">
    <div class="row">
      <% @products.each do |product| %>
        <div class="col-xs-3">
          <%= product.title %>
          <%= product.description %>
          <%= product.status %>
          <%= link_to "Show", product_path(product) %>
          <%= link_to "Offre",  %>
        </div>
      <% end %>
    </div>
  </div>
</div>

